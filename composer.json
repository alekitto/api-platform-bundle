{
    "name": "fazland/api-platform-bundle",
    "license": "MIT",
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Alessandro Chitolina",
            "email": "alekitto@gmail.com"
        },
        {
            "name": "Fazland Team",
            "email": "dev@fazland.com"
        }
    ],
    "require": {
        "justinrainbow/json-schema": "^5.2",
        "kcs/serializer": "^2.0",
        "cakephp/chronos": "^1.1",
        "sensio/framework-extra-bundle": "^3.0|^4.0|^5.0",
        "symfony/cache": "^3.1|^4.0",
        "symfony/debug": "^2.8|^3.0|^4.0",
        "symfony/form": "^2.8|^3.0|^4.0",
        "symfony/framework-bundle": "^2.8|^3.0|^4.0",
        "symfony/http-foundation": "^2.8|^3.0|^4.0",
        "symfony/inflector": "^3.1|^4.0",
        "symfony/security-bundle": "^2.8|^3.0|^4.0",
        "symfony/validator": "^2.8|^3.0|^4.0",
        "symfony/yaml": "^2.8|^3.0|^4.0",
        "willdurand/negotiation": "^2.3"
    },
    "require-dev": {
        "doctrine/annotations": "^1.4",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/orm": "^2.5",
        "symfony/browser-kit": "^2.8|^3.0|^4.0",
        "symfony/phpunit-bridge": "^2.8|^3.0|^4.0",
        "symfony/translation": "^2.8|^3.0|^4.0",
        "symfony/var-dumper": "^2.8|^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Fazland\\ApiPlatformBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fazland\\ApiPlatformBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
