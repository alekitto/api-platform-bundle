{
    "name": "fazland/api-platform-bundle",
    "license": "MIT",
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Alessandro Chitolina",
            "email": "alekitto@gmail.com"
        },
        {
            "name": "Fazland Team",
            "email": "dev@fazland.com"
        }
    ],
    "description": "Build RESTful API within a Symfony project.",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "cakephp/chronos": "^1.1",
        "justinrainbow/json-schema": "^5.2",
        "kcs/metadata": "^2.1",
        "kcs/serializer": "^2.0",
        "sensio/framework-extra-bundle": "^3.0|^4.0|^5.0",
        "symfony/cache": "^3.4|^4.0",
        "symfony/debug": "^3.4|^4.0",
        "symfony/form": "^3.4|^4.0",
        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/http-foundation": "^3.4|^4.0",
        "symfony/inflector": "^3.4|^4.0",
        "symfony/security": "^3.4|^4.0",
        "symfony/validator": "^3.4|^4.0",
        "symfony/yaml": "^3.4|^4.0",
        "willdurand/negotiation": "^2.3"
    },
    "require-dev": {
        "alcaeus/mongo-php-adapter": "^1.1",
        "doctrine/annotations": "^1.4",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/mongodb-odm-bundle": "^3.4",
        "doctrine/orm": "^2.5",
        "fazland/elastica-odm": "dev-master",
        "phpunit/phpunit": "^6.0|^7.0",
        "symfony/browser-kit": "^3.4|^4.0",
        "symfony/phpunit-bridge": "^3.4|^4.0",
        "symfony/security-bundle": "^3.4|^4.0",
        "symfony/translation": "^3.4|^4.0",
        "symfony/var-dumper": "^3.4|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Fazland\\ApiPlatformBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fazland\\ApiPlatformBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "ext-mongo": "1.6.16",
            "ext-mongodb": "1.3.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
